@model InvCore360_App.Model.Models.ConfiguracionNegocio

@{
    ViewData["Title"] = "Eliminar Configuración";
}

<h1>Eliminar Configuración</h1>

<dl class="row">
    <dt class="col-sm-2">NombreNegocio</dt>
    <dd class="col-sm-10">@Model.NombreNegocio</dd>

    <dt class="col-sm-2">Moneda</dt>
    <dd class="col-sm-10">@Model.MonedaPredeterminada</dd>
</dl>

<form asp-action="Delete" method="post">
    <input type="hidden" asp-for="ConfigID" />
    <button type="submit" class="btn btn-danger">Eliminar</button>
    <a class="btn btn-secondary" asp-action="Index">Cancelar</a>
</form>