@model InvCore360_App.Model.Models.AlertaInventario

@{
    ViewData["Title"] = "Detalle Alerta";
}

<h1>Detalle</h1>

<dl class="row">
    <dt class="col-sm-2">Producto</dt>
    <dd class="col-sm-10">@Model.Producto?.Nombre</dd>

    <dt class="col-sm-2">Tipo</dt>
    <dd class="col-sm-10">@Model.TipoAlerta</dd>

    <dt class="col-sm-2">StockActual</dt>
    <dd class="col-sm-10">@Model.StockActual</dd>

    <dt class="col-sm-2">Estado</dt>
    <dd class="col-sm-10">@Model.EstadoAlerta</dd>
</dl>

<a class="btn btn-secondary" asp-action="Index">Back to List</a>