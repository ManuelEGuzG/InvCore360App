@model InvCore360_App.Model.Models.MovimientoInventario

@{
    ViewData["Title"] = "Detalle Movimiento";
}

<h1>Detalle</h1>

<dl class="row">
    <dt class="col-sm-2">Producto</dt>
    <dd class="col-sm-10">@Model.Producto?.Nombre</dd>

    <dt class="col-sm-2">Tipo</dt>
    <dd class="col-sm-10">@Model.TipoMovimiento</dd>

    <dt class="col-sm-2">Cantidad</dt>
    <dd class="col-sm-10">@Model.Cantidad</dd>

    <dt class="col-sm-2">StockAnterior</dt>
    <dd class="col-sm-10">@Model.StockAnterior</dd>

    <dt class="col-sm-2">StockNuevo</dt>
    <dd class="col-sm-10">@Model.StockNuevo</dd>
</dl>

<a class="btn btn-secondary" asp-action="Index">Back to List</a>