@model InvCore360_App.Model.Models.Gasto

@{
    ViewData["Title"] = "Detalle Gasto";
}

<h1>Detalle</h1>

<dl class="row">
    <dt class="col-sm-2">Concepto</dt>
    <dd class="col-sm-10">@Model.Concepto</dd>

    <dt class="col-sm-2">Monto</dt>
    <dd class="col-sm-10">@Model.Monto.ToString("C")</dd>

    <dt class="col-sm-2">Fecha</dt>
    <dd class="col-sm-10">@Model.FechaGasto.ToString("g")</dd>

    <dt class="col-sm-2">Usuario</dt>
    <dd class="col-sm-10">@Model.Usuario?.NombreCompleto</dd>
</dl>

<a class="btn btn-secondary" asp-action="Index">Back to List</a>